   static    WarehouseRef.Contract {
      label = "Site";
   }
   lov WarehouseRef with  WarehouseSelector {
      label = "Warehouse ID";
   }
   lov CustomerInfoRef with CustomerInfoSelector {
      label = "Customer ID";
   }
   static CustomerInfoRef.Name;
   field Alias;
}
