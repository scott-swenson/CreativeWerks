      label = "Warehouse Alias";
      page CWarehouseAliasOverview;
   }
}

-------------------------------- MAIN PAGES ---------------------------------
page CWarehouseAliasOverview using CWarehouseAliasSet {
   label = "Warehouse Alias";
   list CWarehouseAliasList;
}

