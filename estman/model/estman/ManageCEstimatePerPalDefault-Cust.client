list CEstimatePerPalDefaultList for CEstimatePerPalDefault {
   orderby = Contract, OverheadType;
   static Company;
   lov CompanySiteRef with ReferenceUserAllowedSiteLovSelector {
      label = "Site";
      description = CompanySiteRef.ContractDesc;
      validate command {
         execute {
            set Company = CompanySiteRef.Company;
         }
      }
      preview = UserAllowedSiteLovCard;
   }
   field OverheadType;
   lov CostElementRef with ProjectCostElementSelector {
      label = "Cost Element";
      description = CostElementRef.Description;
   }
   field AmountPerPallet;
}

