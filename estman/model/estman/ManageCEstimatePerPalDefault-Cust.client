   orderby = Contract, OverheadType;
   static Company;
   lov CompanySiteRef with CompanySiteSelector {
      label = "Site";
      description = CompanySiteRef.Name;
      validate command {
         execute {
            set Company = CompanySiteRef.Company;
         }
      }
   }
   field OverheadType;
   lov CostElementRef with ProjectCostElementSelector {
      label = "Cost Element";
      description = CostElementRef.Description;
   }
   field AmountPerPallet;
}

